<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>
<name>min()</name>

<category>Math</category>

<subcategory>Calculation</subcategory>

<usage>Web &amp; Application</usage>

<example>
<image></image>
<code><![CDATA[
int d = min(5, 9);            // Sets "d" to 5
int e = min(-4, -12);         // Sets "e" to -12
float f = min(12.3, 230.24);  // Sets "f" to 12.3
]]></code>
</example>

<example>
<image></image>
<code><![CDATA[
int[] list = { 5, 1, 2, -3 };
int h = min(list);            // Sets "h" to -3
]]></code>
</example>

<description><![CDATA[Determines the smallest value in a sequence of numbers.]]></description>

<syntax><![CDATA[
min(<kbd>value1</kbd>, <kbd>value2</kbd>)
min(<kbd>value1</kbd>, <kbd>value2</kbd>, <kbd>value 3</kbd>)
min(<kbd>array</kbd>)
]]></syntax>

<parameter>
<label>value1</label>
<description><![CDATA[int or float]]></description>
</parameter>

<parameter>
<label>value2</label>
<description><![CDATA[int or float]]></description>
</parameter>

<parameter>
<label>value3</label>
<description><![CDATA[int or float]]></description>
</parameter>

<parameter>
<label>array</label>
<description><![CDATA[int or float array]]></description>
</parameter>

<returns>int or float (depending on the inputs)</returns>

<related>
max()
</related>

