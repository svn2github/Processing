<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<root>
<name>noSmooth()</name>

<category>Shape</category>

<subcategory>Attributes</subcategory>

<example>
<image>noSmooth_.png</image>
<code><![CDATA[
background(0);
smooth();
ellipse(70, 48, 36, 36);
noSmooth();
ellipse(30, 48, 36, 36);
]]></code>
</example>

<description><![CDATA[
Draws all geometry with jagged (aliased) edges.
]]></description>

</root>
